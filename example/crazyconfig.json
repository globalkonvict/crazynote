{
  "directory": ".",
  "ignore": [
    "**/.env",
    ".**/",
    ".git/**",
    "**/node_modules/**",
    "**/package-lock.json",
    "dist/**",
    "**/*test*",
    "template.hbs",
    "output.md",
    "output.json",
    "crazyconfig.json",
    "example/**"
  ],
  "outputDir": "./",
  "outputFileName": "output",
  "templatePath": "template.hbs",
  "exclusionText": " (excluded)",
  "removeIgnoredFromTree": true,
  "includeAST": true
}
